<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1836.app.ui.newWorkoutController">
   <top>
      <Label text="Add new workout" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </top>
   <center>
      <FlowPane alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Button fx:id="CardioBtn" mnemonicParsing="false" onAction="#goCardioWorkout" prefHeight="100.0" prefWidth="120.0" style="-fx-background-radius: 20;" text="Cardio">
               <FlowPane.margin>
                  <Insets left="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="StrengthBtn" mnemonicParsing="false" onAction="#goStrengthWorkout" prefHeight="100.0" prefWidth="120.0" style="-fx-background-radius: 20;" text="Strength">
               <FlowPane.margin>
                  <Insets left="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <padding>
            <Insets top="50.0" />
         </padding>
      </FlowPane>
   </center>
</BorderPane>
