<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tdt4140.gr1836.app.ui.AvailableCoachesController">
   <top>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="10.0">
               <children>
                  <Label prefHeight="77.0" prefWidth="193.0" text="Available coaches">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Separator prefWidth="200.0" />
                  <TableView fx:id="view" prefHeight="597.0" prefWidth="1004.0">
                    <columns>
                      <TableColumn fx:id="name" prefWidth="245.0" text="Name" />
                      <TableColumn fx:id="city" minWidth="0.0" prefWidth="236.0" text="City" />
                        <TableColumn fx:id="age" prefWidth="262.0" text="Age" />
                        <TableColumn fx:id="email" prefWidth="260.0" text="Mail" />
                    </columns>
                  </TableView>
                  <Button fx:id="homeBtn" alignment="CENTER" mnemonicParsing="false" onAction="#goHome" text="Back" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
